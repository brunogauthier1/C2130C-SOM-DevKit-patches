+static void __exit akm_compass_exit(void)
+{
+    printk("%s:%d, ACHEUL\n", __FUNCTION__, __LINE__);
+	pr_info("%s:%d, AKM compass driver: release.", __FUNCTION__, __LINE__);
+	i2c_del_driver(&akm_compass_driver);
+}
