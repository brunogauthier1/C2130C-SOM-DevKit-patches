+	uint8_t rbuf[AKM_SENSOR_DATA_SIZE];
+
+	mutex_lock(&akm->sensor_mutex);
+	memcpy(&rbuf, akm->sense_data, sizeof(rbuf));
+	mutex_unlock(&akm->sensor_mutex);
+
+	return akm_buf_print(buf, rbuf, AKM_SENSOR_DATA_SIZE);
+}
