+static int akm_pinctrl_init(struct akm_compass_data *akm)
+{
+	struct i2c_client *client = akm->i2c;
+    printk("%s:%d, ACHEUL", __FUNCTION__, __LINE__);
+
+	akm->pinctrl = devm_pinctrl_get(&client->dev);
