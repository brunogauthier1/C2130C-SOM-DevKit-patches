+static ssize_t akm_bin_accel_write(
+	struct file *file,
+	struct kobject *kobj,
+	struct bin_attribute *attr,
+		char *buf,
+		loff_t pos,
+		size_t size)
+{
+	struct device *dev = container_of(kobj, struct device, kobj);
+	struct akm_compass_data *akm = dev_get_drvdata(dev);
+	int16_t *accel_data;
