+	akm->accel_data[1] = accel_data[1];
+	akm->accel_data[2] = accel_data[2];
+	mutex_unlock(&akm->accel_mutex);
+
+	dev_vdbg(&akm->i2c->dev, "accel:%d,%d,%d\n",
+			accel_data[0], accel_data[1], accel_data[2]);
+
+	return size;
+}
